{
  "$schema": "https://schema.tauri.app/config/2.0.0-rc",
  "productName": "vivi-paste",
  "version": "0.1.0",
  "identifier": "com.tauri.dev",
  "build": {
    "frontendDist": "../dist",
    "devUrl": "http://localhost:5173",
    "beforeDevCommand": "npm run dev",
    "beforeBuildCommand": "npm run build"
  },
  "app": {
    "windows": [
      {
        "label": "main",
        "title": "vivi-paste",
        "width": 400,
        "height": 300,
        "resizable": false
      }
    ],
    "security": {
      "csp": null
    }
  },
  "bundle": {
    "createUpdaterArtifacts": true,
    "active": true,
    "targets": "all",
    "icon": [
      "icons/32x32.png",
      "icons/128x128.png",
      "icons/128x128@2x.png",
      "icons/icon.icns",
      "icons/icon.ico"
    ]
  },
  "plugins": {
    "updater": {
      "pubkey": "dW50cnVzdGVkIGNvbW1lbnQ6IG1pbmlzaWduIHB1YmxpYyBrZXk6IEUyNjcyNjNBMUVCNTBBMTMKUldRVENyVWVPaVpuNGpJeXFZdWZwQmxjOTVQM2JqaE5XNms1ODBYYjdPVmYvZmozVmNUK1RaVHkK"
    }
  }
}
